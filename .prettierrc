{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/hooks/(.*)$",
    "^@/context/(.*)$",
    "^@/components/(.*)$",
    "^@/utils/(.*)$",
    "^@/data/(.*)$",
    "^@/public/(.*)$",
    "^@/types/(.*)$",
    "^./*.*.type",
    "^./*.*.scss",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
